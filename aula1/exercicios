#include <iostream> 
using namespace std;
int main()
{
	int A,B,C,D;
	cin >> A;
	cin >> B;
	cin >> C;
	cin>> D;
    cout<<"DIFERENÇA = "<<(A*B-C*D)<<endl;
	
	return 0;
}
