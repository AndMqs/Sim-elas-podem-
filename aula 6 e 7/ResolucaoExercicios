1)

#include <bits/stdc++.h>

using namespace std;

int main(){
  
  double a, soma = 0, qtd = 0;
  cin >> a;
  
  while (a >= 0){
    soma = soma + a; 
    qtd++;
    cin >> a;
  }
  double media = soma/qtd;
  cout << setprecision(2) << fixed << media << endl;
    return 0;
}
—-----------------------------------------------------------------------------------

2)

#include <bits/stdc++.h>

using namespace std;

int main(){
  int a;
  cin >> a;

  for (int i = 1; i <= a; i++){
    if(a % i == 0){
      cout << i << endl;
    }
  }

  
    return 0;
}
—--------------------------------------------------------------------------------

3)

#include <bits/stdc++.h>

using namespace std;
 
int main() {
  int a; 
  int gasolina = 0; 
  int diesel = 0; 
  int alcool = 0;
  cin>>a;
  while (a!=4) {
    if(a == 1)
        alcool++;
    else if(a == 2)
        gasolina++;
    else if(a == 3)
        diesel++;
        cin>>a;
    }
    cout <<"MUITO OBRIGADO"<<endl;
    cout << "Alcool: " << alcool << endl;
    cout << "Gasolina: " << gasolina << endl;
    cout <<"Diesel: " << diesel << endl;
    return 0;
}

—--------------------------------------------------------------------------------------

4)

#include <bits/stdc++.h>

using namespace std;

int main(){
  int a;
  cin >> a;

  for (int i = 1; i <= a; i++){
      cout << i << endl;
  }

  
    return 0;
}

